float average(float* values, int n_values) {    float cpt = *values;    int somme=0;    int i;        for (int i=0; i<n_values;i++)    {        somme += *values;    }    cpt= somme/n_values;    return cpt;}
#include <stdlib.h>#include <stdlib.h>float average(float*values, int n_values){    float sum;    int i;    for(i=0; i<n_values; i++)    {        sum += *(values + i);    }    sum /= n_values;    return sum;}int main(){    int n_values = 5;    float* values = (float*) malloc(n_values*sizeof(float));    for (int i=0; i<n_values; ++i) {        *(values+i) = i+2;        }  printf("Moyenne tableau : %f\n", average(values, n_values));}
float average (float *values, int n_values){    int i;    float moy;    int res;    moy = *values;        for ( i = 0; i < n_values; i++)    {        moy = moy + (*(values + i));    }   res =  moy/n_values;    return res;}float test_min_floats(){    int n_values = 0;    int i;    float *values;        printf("Combien de valeurs ?");    scanf("%d", &n_values);        values = (float*)malloc(sizeof(float) * n_values );        for (i = 0 ; i < n_values ; i++)    {        printf("donnez la valeur %d",i);        scanf("%d",*(values+i));    }        free(values);        return 0;}
