#include <stdio.h>#include <stdlib.h>float average(float*values, int n_values) {    int taille, i, val;    int *tab=NULL;    tab = (int*)malloc(n_values*sizeof(int));    for (i=0; i<n_values; i++) {        printf("Entrer une valeur du tableau");        scanf("%d", &val);        *(&tab + i) = &val;        *values += val;    }    printf("%d", &values/n_values);    free(tab);}
float average(float *values, int n_values){    int sum = 0, i;    for (i = 0; i < n_values; i++){        sum += *(values + i);            }    sum /= (n_values-1);    return sum;    }
float average(float* values, int n_values){    float moy;    if (values != NULL){        moy = 0;    }    for (int i = 0; i < n_values; i++){        moy = moy + *(values+i);    }    moy=(moy/ n_values );    return moy;}void test_average(){    int n, i;    printf("Nombre d'elements : ");    scanf("%d", &n);    float tab = (float) malloc(n * sizeof(float));    if (tab == NULL){        printf("Pas de mémoire disponible ! Erreur !");    }    else{        printf("Valeur : ");        for (i = 0; i < n; i++){            scanf("%f", tab+i);        }        printf("Moyenne du tableau : %f", average(tab, n));    }}
