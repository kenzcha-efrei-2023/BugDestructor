float average(float* values, int n_values){    float moy=0;    if (value != NULL){        moy = 0;    }    for(int i = 0 ; i < n_values ; i++){        moy = moy + *(values+i);    }    moy = moy / n_values;    return moy;}
int float_average(float*values, int n_values) {// Déclaration variables //    int i; float Moy = 0;/*Algorithme */    for (i=0; i<n_values; i++) {        Moy += *(values+i) ;    }    printf("La moyenne du tableau est: %.2f", Moy/n_values); // Affiche le float avec 2 chiffres après la virgule.    free(values);    return 0; }
float average(float* values, int n_values){    float moy=0;    if (values != NULL){        moy = 0;    }    for(int i = 0 ; i < n_values ; i++){        moy = moy + *(values+i);    }    moy = moy / n_values;    return moy;}
float average(float*values, int n_values) {// Déclaration variables //    int i; float Moy = 0;/*Algorithme */    for (i=0; i<n_values; i++) {        Moy += (float)*(values+i) ;    }    printf("La moyenne du tableau est: %.2f", Moy/n_values); // Affiche le float avec 2 chiffres après la virgule.    free(values);    return 0; }
float average(float*values, int n_values) {// Déclaration variables //    int i; float Moy = 0;/*Algorithme */    for (i=0; i<n_values; i++) {        Moy += (float)*(values+i) ;    }    printf("La moyenne du tableau est: %.2f", Moy/n_values); // Affiche le float avec 2 chiffres après la virgule.    return Moy/n_values; }
float average(float* values, int n_values) {float moyenne=0.0;int i;if(values!=NULL){for(i=0;i<n_values;i++){moyenne += *(values+i+1);}}moyenne = moyenne/i;return moyenne;}
