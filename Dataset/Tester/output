float average (float *values, int n_values){    int i;    float moy;    moy = *values;        for ( i = 0; i < n_values; i++)    {        moy = moy + (*(values + i));    }    float res = moy/n_values;    return res;}void test_average(){    float result;    float *T;    int Tl=3;    T = (float*) malloc (Tl * sizeof(float));    *T = 1;    *(T+1)=2;    *(T+2)=3;    result = average(T, Tl);    printf ("%lf", result);}
