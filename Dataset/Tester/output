float average(float* values, int n_values){    int moy = 0, somme = 0;    for(int i = 0; i < n_values; i++){        somme += i        *moy = somme / *n_values;    }}
float average(float*values, int n_values){    int moy=0;    for (int i=0; i<n_values; i++){        moy = moy + *(values+i);    }    return (moy/n_values);}
float average(float*values, int n_values){    float moy=0;    for (int i=0; i<n_values; i++){        moy = moy + *(values+i);    }    return (moy/n_values);}
float average(float* values, int n_values){    int moy = 0, somme = 0;    for(int i = 0; i < n_values; i++){        somme += i        *moy = somme / *(n_values+i);    }}
float average(float* values, int n_values){    int moy = 0, somme = 0;    for(int i = 0; i < n_values; i++){        somme += i;        moy = somme / *(n_values+i);    }}
float average(float *tableau, int tailleTableau){    int moy = 0, somme = 0;    for(int i = 0; i < tailleTableau; i++){        somme += i;        moy = somme / tailleTableau;    }}
float average(float *tableau, int tailleTableau){    int moy = 0, somme = 0;    for(int i = 0; i < tailleTableau; i++){        somme += i;        moy = somme / tailleTableau;    }    return moy;}
float average(float *tableau, int tailleTableau){    int moy = 0, somme = 0;    for(int i = 0; i < tailleTableau; i++){        somme += *(tableau + i);        moy = somme / tailleTableau;    }    return moy;}
float average(float *tableau, int tailleTableau){    float moy = 0, somme = 0;    for(int i = 0; i < tailleTableau; i++){        somme += *(tableau + i);        moy = somme / tailleTableau;    }    return moy;}
float average(float*values,int n_values){    float sum;    int cpt;    sum = 0;    for(cpt=0;cpt<n_values;cpt++){        sum += *(values+cpt);    }    return sum/n_values;}
float average(float* values, int n_values){    float sum;    int i;    for (i = 0; i < n_values; i++){        sum += *(values + i);    }    return sum / n_values;}
float average(float* values, int n_values){    float sum = 0.0;    int i;    for (i = 0; i < n_values; i++){        sum += *(values + i);    }    return sum / n_values;}
float average(float *tableau, int tailleTableau){    float moy = 0, somme = 0;    for(int i = 0; i < tailleTableau; i++){        somme += *(tableau + i);        moy = somme / tailleTableau;    }    return moy;}
float average(float* values, int n_values){    float moy = 0, somme = 0;    for (int i=0; i < n_values; i++){        somme += *(values + i);        moy = somme/n_values;    }}
